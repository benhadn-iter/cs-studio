<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.csstudio.channel.application.category"
            name="Channel Applications">
      </category>
      <view
            category="org.csstudio.channel.application.category"
            class="org.csstudio.rocs.views.ROCS"
            icon="platform:/plugin/org.csstudio.rocs.widgets/icons/rocs-16.png"
            id="org.csstudio.rocs.views.ROCS"
            name="ROCS"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:display">
         <separator
               name="org.csstudio.channel.views.before"
               visible="true">
         </separator>
         <command
               commandId="org.eclipse.ui.views.showView"
               icon="platform:/plugin/org.csstudio.rocs.widgets/icons/rocs-16.png"
               label="ROCS"
               style="push">
            <parameter
                  name="org.eclipse.ui.views.showView.viewId"
                  value="org.csstudio.rocs.views.ROCS">
            </parameter>
         </command>
         <separator
               name="org.csstudio.channel.views.after"
               visible="true">
         </separator>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.csstudio.ui.menu.popup.channelquery">
         <command
               commandId="org.csstudio.rocs.views.OpenROCS"
               icon="platform:/plugin/org.csstudio.rocs.widgets/icons/rocs-16.png"
               label="ROCS"
               style="push">
         </command>
      </menuContribution>
    </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.csstudio.rocs.views.OpenROCS"
            id="org.csstudio.rocs.views.OpenROCS"
            name="ROCS">
      </command>
   </extension>
</plugin>
